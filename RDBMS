alter session set current_schema=bank;
SET heading OFF;
SELECT B.CUST_ID, B.CUST_FNAME , B.CUST_LNAME , C.CUST_PHONE , C.CUST_CITY
FROM BANK_CUSTOMER B INNER JOIN BANK_CUST_CONTACT C
ON B.CUST_ID=C.CUST_ID
WHERE NOT CUST_STATE='Karnataka'
AND CONTACT_TYPE='HOME'
GROUP BY B.CUST_ID, B.CUST_FNAME , B.CUST_LNAME , C.CUST_PHONE , C.CUST_CITY;
